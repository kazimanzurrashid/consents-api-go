{
  "name": "consents-go",
  "dockerComposeFile": "docker-compose.yml",
  "service": "api",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "go.toolsManagement.checkForUpdates": "local",
        "go.useLanguageServer": true,
        "go.gopath": "/go",
        "go.goroot": "/usr/local/go"
      },
      "extensions": [
        "golang.Go",
        "EditorConfig.EditorConfig"
      ]
    }
  },
  "forwardPorts": [
    8080,
    5432
  ]
}
